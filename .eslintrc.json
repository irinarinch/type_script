{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "standard-with-typescript",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    },
    "plugins": [
       "jest"
    ]
}
